# Prefer storing secrets in config/credentials.yml.enc; use .env for local overrides or prototyping.
GITHUB_APP_ID=
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GITHUB_PRIVATE_KEY=
# Optional fallback if you keep the PEM on disk instead of credentials
GITHUB_PRIVATE_KEY_PATH=
GITHUB_INSTALLATION_ID=
GITHUB_WEBHOOK_SECRET=
GITHUB_CALLBACK_URL_PROD=
# e.g. http://127.0.0.1:3000/auth/github/callback or your tunneled URL
GITHUB_CALLBACK_URL_DEV=
GITHUB_SETUP_URL=
RESEND_API_KEY=
DO_SPACES_ACCESS_KEY_ID=
DO_SPACES_SECRET_ACCESS_KEY=
DO_SPACES_ENDPOINT=
DO_SPACES_CDN=
# Optional override; defaults to ai_studio when GEMINI_API_KEY is set
# GEMINI_PROVIDER=ai_studio
GOOGLE_CLOUD_PROJECT=
GEMINI_LOCATION=us-central1
GEMINI_API_KEY=
GEMINI_API_BASE=https://generativelanguage.googleapis.com/v1beta
GENERATED_IMAGE_UPLOAD=
APP_HOST=http://127.0.0.1:3000
# Optional basic auth for jobs UI (if/when mounted)
MISSION_CONTROL_JOBS_HTTP_BASIC=loftwah:hunter2
KAMAL_REGISTRY_PASSWORD=
WEB_HOSTS=techub-do
ALERT_EMAIL=
# Backups (DigitalOcean Spaces / S3-compatible)
# If using same Space as Active Storage, you can omit BACKUP_BUCKET and it will fall back to DO_SPACES_BUCKET.
BACKUP_BUCKET=
BACKUP_PREFIX=db_backups
BACKUP_RETENTION_DAYS=14
BACKUP_KEEP_MIN=7
